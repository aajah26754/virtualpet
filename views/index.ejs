<%- include('partials/header', { user: user }) %>
<h1>Welcome</h1>
<% if (user) { %>
  <p>Logged in as <strong><%= user.username %></strong></p>
  <p><a href="/profile">Go to profile</a></p>
<% } else { %>
  <p>You are not logged in. <a href="/login">Login</a></p>
<% } %>
